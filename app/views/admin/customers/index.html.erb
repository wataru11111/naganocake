<div class="container">

<h1>会員一覧</h1>

<table class="table">
  <thead>
    <tr>
      <th>会員ID</th>
      <th>氏名</th>
      <th>メールアドレス</th>
    </tr>
 </thead>

<tbody>
<% @customers.each do |customers| %>
<tr>
<td>
<%= customers.id %>
</td>
<td>
<%= link_to admin_customer_path(customer.id) do %>
<%= customer.name %>
<% end %>
</td>
<td>
<%= customer.email %>
<% end %>
</td>
</tr>
</tbody>
</table>
<%= paginate @customer %>
</div>